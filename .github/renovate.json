{
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "automerge": true
    },
    {
      "matchDepTypes": [
        "devDependencies"
      ],
      "automerge": true
    }
  ],
  "labels": [
    "dependencies",
    "maintenance"
  ],
  "rangeStrategy": "update-lockfile",
  "requiredStatusChecks": null
}
